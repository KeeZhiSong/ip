  <children>
    <Label fx:id="dialog" text="Label" wrapText="true" maxWidth="300.0" minHeight="-Infinity">
         <padding>
            <Insets bottom="8.0" left="12.0" right="12.0" top="8.0" />
         </padding>
         <HBox.margin>
            <Insets bottom="2.0" left="4.0" right="4.0" top="2.0" />
         </HBox.margin>
    </Label>
    <ImageView fx:id="displayPicture" fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" />
  </children>
  <padding>
    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
  </padding>
</fx:root>
